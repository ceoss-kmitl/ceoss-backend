{
  "name": "ceoss-kmitl-backend",
  "version": "1.1.1",
  "description": "Backend of CEOSS project",
  "main": "build/main.js",
  "scripts": {
    "start": "NODE_ENV=production node build/main.js",
    "build": "rm -rf build && tsc --project ./tsconfig.json && tscpaths -p ./tsconfig.json -s ./src -o ./build",
    "dev": "nodemon",
    "lint": "eslint src/**/*.ts --fix",
    "typeorm": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli -f src/configs/ormconfig-migration.ts",
    "model:gen": "npm run typeorm -- migration:generate -n",
    "model:up": "npm run typeorm -- migration:run",
    "model:revert": "npm run typeorm -- migration:revert"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ceoss-kmitl/backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ceoss-kmitl/backend/issues"
  },
  "homepage": "https://github.com/ceoss-kmitl/backend#readme",
  "devDependencies": {
    "@types/chalk": "2.2.0",
    "@types/cors": "2.8.10",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.12",
    "@types/lodash": "4.14.178",
    "@types/morgan": "1.9.2",
    "@types/nanoid": "3.0.0",
    "@types/node": "15.12.4",
    "@types/node-fetch": "2.5.12",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "nodemon": "2.0.7",
    "prettier": "2.3.2",
    "ts-node": "10.0.0",
    "tsconfig-paths": "3.9.0",
    "tscpaths": "0.0.9",
    "typescript": "4.3.4"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "chalk": "4.1.1",
    "cheerio": "1.0.0-rc.10",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "cors": "2.8.5",
    "dayjs": "1.10.7",
    "dotenv": "10.0.0",
    "exceljs": "4.2.1",
    "express": "4.17.1",
    "google-auth-library": "7.11.0",
    "iconv-lite": "0.6.3",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "multer": "1.4.2",
    "nanoid": "3.1.23",
    "node-fetch": "2.6.1",
    "pg": "8.6.0",
    "puppeteer": "13.3.2",
    "reflect-metadata": "0.1.13",
    "routing-controllers": "0.9.0",
    "typeorm": "0.2.34",
    "typeorm-naming-strategies": "2.0.0"
  }
}
